{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "tsBuildInfoFile": ".tsc-cache/mobile.tsbuildinfo"
  },
  "include": ["src"],
  "references": [{ "path": "../shared" }]
}